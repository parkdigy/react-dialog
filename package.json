{
  "name": "@pdg/react-dialog",
  "title": "React Dialog",
  "type": "module",
  "version": "1.0.31",
  "description": "React Dialog",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkdigy/react-dialog.git",
    "baseUrl": "https://github.com/parkdigy/react-dialog"
  },
  "bugs": {
    "url": "https://github.com/parkdigy/react-dialog/issues"
  },
  "homepage": "https://github.com/parkdigy/react-dialog#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "cd examples && npm run dev",
    "dev-prd-lib": "cd examples && npm run dev-prd-lib",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:examples": "cd examples && npm run build",
    "pub": "npm i && npm run build:examples && npm run build && npm publish --access=public && rm ./.git/hooks/pre-commit",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "reinstall-module": "rm -rf node_modules && rm -f package-lock.json && npm i"
  },
  "author": "YOUNG CHUL PARK",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "react",
    "dialog",
    "mui",
    "typescript",
    "javascript"
  ],
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@pdg/react-hook": "^1.0.30"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
